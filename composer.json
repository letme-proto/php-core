{
    "name":"letmeproto/core",
    "type": "library",
    "keywords": ["prototype", "laravel", "pdo", "psr", "psr-11", "psr-7", "http-message", "container", "container-interop", "container-interface", "php7.2", "fast prototype", "fast app"],
    "description":"LetMeProto IoC Core engine for fast prototype apps",
    "require":{
        "php": "^7.2",
        "psr/http-message": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/container": "^1.0",
        "grajewsky/annotations": "^1.1"
    },
    "require-dev":{
        "phpunit/phpunit": "^8.0"
    },
    "scripts": {
        "tests": "phpunit --bootstrap ./vendor/autoload.php --testdox tests"
    },
    "autoload":{
        "psr-4":{
            "LetMeProto\\": "src/",
            "Test\\": "tests/"
        }
    }

}
